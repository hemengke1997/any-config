{
  "name": "@minko-fe/any-config",
  "version": "1.2.0",
  "description": "minko's project config ✅",
  "keywords": [
    "eslint",
    "prettier",
    "commitlint",
    "tsconfig",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/any-config.git"
  },
  "author": "hemengke1997 <https://github.com/hemengke1997>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "upgrade:deps": "pnpm update --i --L",
    "//": "然后再手动发包",
    "before:release": "bumpp package.json packages/*/package.json packages/eslint/*/package.json --commit --no-tag",
    "release": "pnpm -r publish --access public"
  },
  "devDependencies": {
    "@minko-fe/commitlint-config": "workspace:*",
    "@minko-fe/eslint-config": "workspace:*",
    "@minko-fe/prettier-config": "workspace:*",
    "@minko-fe/tsconfig": "workspace:*",
    "bumpp": "^8.2.1",
    "prettier": "^2.7.1"
  }
}
