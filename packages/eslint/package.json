    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.3.1",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.11.1",
