{
  "name": "@minko-fe/eslint-config",
  "version": "4.1.0",
  "description": "eslint-config",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/any-config.git"
  },
  "author": "hemengke1997 <https://github.com/hemengke1997>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
